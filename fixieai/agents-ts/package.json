    "dprint": "^0.35.3",
    "eslint": "^8.37.0",
    "eslint-config-nth": "^2.0.1",
    "nodemon": "^2.0.21",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "express": "^4.18.2"
