{
  "name": "@fixie/sdk",
  "private": true,
  "version": "1.0.0",
  "description": "The Fixie SDK (https://fixie.ai/)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "/src"
  ],
  "scripts": {
